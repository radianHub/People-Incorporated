<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Physical_Health_Concerns</fullName>
    <active>true</active>
    <description>Requires description for physical health concerns</description>
    <errorConditionFormula>AND(Physical_Health_Concerns__c = True,
ISBLANK(Physical_Health_Concerns_Description__c))</errorConditionFormula>
    <errorDisplayField>Physical_Health_Concerns_Description__c</errorDisplayField>
    <errorMessage>Physical Health Concerns Description required.</errorMessage>
</ValidationRule>
